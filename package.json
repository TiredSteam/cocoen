  "homepage": "https://github.com/koenoe/cocoen#readme",
  "bugs": {
    "url": "https://github.com/koenoe/cocoen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koenoe/cocoen.git"
